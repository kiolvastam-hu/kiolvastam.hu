{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "dev",
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"isBackground": true,
			"problemMatcher": {
				"owner": "custom",
				"pattern": {
					"regexp": "^$"
				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": "next",
					"endsPattern": "started server on.*"
				}
			},
			// "options": {
			// 	"cwd": "${workspaceFolder}/apps/web"
			// }
		}
	]
}